{
  "name": "task-forge",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "frontend": "vite",
    "backend": "npx supabase start --exclude imgproxy,edge-runtime,logflare,realtime",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "start": "npm run dev",
    "stop": "npx supabase stop",
    "build": "tsc -b && vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/ibm-plex-mono": "^5.2.6",
    "@fontsource/permanent-marker": "^5.2.5",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.74.3",
    "@tanstack/react-query-devtools": "^5.74.3",
    "react": "latest",
    "react-dom": "latest",
    "react-error-boundary": "^6.0.0",
    "react-router-dom": "^6.30.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "latest",
    "concurrently": "^9.2.0",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "prettier": "3.5.3",
    "supabase": "^2.48.3",
    "typescript": "^5.9.3",
    "vite": "latest"
  }
}
